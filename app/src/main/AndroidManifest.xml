<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mydemo">

    <!-- 用于访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:replace="android:icon, android:theme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CountDownActivity" />
        <activity android:name=".eventbus.EventoneActivity" />
        <activity android:name=".eventbus.EventTwoActivity" />
        <activity android:name=".thread.MultiThreadActivity" />
        <activity
            android:name=".design.DesignActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".picasso.PicassoActivity"></activity>
    </application>

</manifest>